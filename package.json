{
  "name": "ow-leaflet-iiif",
  "version": "1.0.3",
  "description": "A Leaflet plugin for viewing IIIF sources",
  "main": "./lib/umd/leaflet-iiif.min.js",
  "module": "./lib/esm/",
  "typings": "./lib/esm/",
  "exports": {
    "./": "./lib/esm"
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "lint": "esw . --ext .ts",
    "compile": "tsc",
    "build": "npm run clean && npm run lint && npm run compile && webpack --progress",
    "start": "npm run build  && concurrently \"npm run compile -- -w\" \"npm run lint -- -w\" \"webpack serve\" ",
    "test": "TS_NODE_PROJECT='./tsconfig.test.json' mocha --require jsdom-global/register --require ts-node/register ./test/unit/**/*.ts",
    "e2e:test": "TS_NODE_PROJECT='./tsconfig.test.json' mocha --require ts-node/register ./test/e2e/**/*.spec.ts --exit",
    "e2e:generate": "TS_NODE_PROJECT='./tsconfig.test.json' node -r ts-node/register test/e2e/generate.ts",
    "site": "rimraf docs/ && typedoc && cpy examples docs/examples && cpy lib/umd docs/lib/umd && cpy lib/leaflet* docs/lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sim51/Leaflet-IIIF.git"
  },
  "keywords": [
    "Leaflet",
    "IIIF",
    "map",
    "zoom"
  ],
  "author": "Jack Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sim51/Leaflet-IIIF/issues"
  },
  "homepage": "https://sim51.github.io/Leaflet-IIIF/",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@types/leaflet": "^1.7.0",
    "@types/mocha": "^8.2.2",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.0.2",
    "cpy-cli": "^3.1.1",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-watch": "^7.0.0",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "leaflet": "^1.7.1",
    "mini-css-extract-plugin": "^1.5.0",
    "mocha": "^8.3.2",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "leaflet": "^1.7.1"
  }
}
